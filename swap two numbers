#include<stdio.h>
void swap(int x,int y);

int main()
{
    int a,b;
    printf("Enter two numbers:");
    scanf("%d %d",&a,&b);
    printf("\n\nBefore swapping :a=%d  b=%d",a,b);
    swap(a,b);
    return 0;
}
void swap(int x,int y)
{
    int t;
    t=x;
    x=y;
    y=t;
    printf("\n\nAfter swapping:a=%d  b=%d\n",x,y);

}
